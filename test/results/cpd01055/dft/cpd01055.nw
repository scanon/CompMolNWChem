xyz cpd01055_geom
end

set lindep:n_dep 0

dft
  maxiter 100
  xc b3lyp
  mulliken
  print "mulliken ao"
  print "final vectors analysis"
end

task dft optimize ignore
property
 dipole
end

task dft property
